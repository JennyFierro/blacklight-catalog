  <div class="card-body">
    <div class="lightbulb">
    <span class="rounded-lightbulb">
    <%= image_tag("lightbulb.svg", :height => "32", :width => "32") %>
    </span>
    </div>
    <p class="text-other-resources"><%= t('blacklight.other_resources.card_descriptor.search_tool') %></p>
